<idea-plugin>
    <id>org.tbm98.flutter-riverpod-snippets</id>
    <name>Flutter Riverpod Snippets</name>
    <vendor email="tbm98.dev@gmail.com" url="https://tbm98.dev">tbm98</vendor>

    <!-- Product and plugin compatibility requirements -->
    <!-- https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="/liveTemplates/Flutter_riverpod_snippets.xml"/>
        <liveTemplateContext implementation="com.github.tbm98.flutterriverpodsnippets.FlutterContext" />
    </extensions>

</idea-plugin>
