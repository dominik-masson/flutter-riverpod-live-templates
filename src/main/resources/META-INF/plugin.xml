<idea-plugin>
    <id>org.tbm98.flutter-riverpod-snippets</id>
    <name>Flutter Riverpod Snippets</name>
    <vendor email="tbm98.dev@gmail.com" url="http://tbm98.github.io">tbm98</vendor>
    <idea-version since-build="202.5103.13" />
    <description><![CDATA[
    The snippets makes using flutter riverpod more simple.
    It support auto generate some snippets while developers coding in Flutter project and them use Flutter riverpod package.
    ]]></description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <defaultLiveTemplates file="/liveTemplates/Flutter_riverpod_snippets.xml"/>
        <liveTemplateContext implementation="FlutterContext" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>